{
  "{layout='...'}": {
    "scope": "html.ee",
    "prefix": "layout",
    "body": "{layout='$1'}"
  },
  "{layout:set name='...' value='...'}": {
    "scope": "html.ee",
    "prefix": "layout-set",
    "body": "{layout:set name='$1' value='$2'}$0"
  },
  "{layout:set name='...'}...{/layout:set}": {
    "scope": "html.ee",
    "prefix": "layout-set-pair",
    "body": "{layout:set name='$1'}$2{/layout:set}$0"
  },
  "{layout:(...)}": {
    "scope": "html.ee",
    "prefix": "layout-var",
    "body": "{layout:$1}"
  },
  "{layout:(...)}...{/layout:(...)}": {
    "scope": "html.ee",
    "prefix": "layout-pair",
    "body": [
      "{layout:$1}",
      "\t$2",
      "{layout:$1}"
    ]
  },
  "{layout:set:append name='...'}...{/layout:set:append}": {
    "scope": "html.ee",
    "prefix": "layout-append",
    "body": "{layout:set:append name='$1'}$2{/layout:set:append}"
  }
}